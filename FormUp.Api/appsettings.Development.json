{
  "ConnectionStrings": {
    "Database": "Server=localhost,1433; Database=FormUp_Dev; User=sa; Password=Example_123; TrustServerCertificate=True"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
